{
    "name": "@xylobyte/hrnet-react-modal",
    "version": "1.0.0",
    "description": "A react modal for HRnet project",
    "keywords": [
        "modal",
        "react",
        "hrnet",
        "dialog"
    ],
    "license": "MIT",
    "author": "Xylobyte",
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
