    "peerDependencies": {
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.32.0",
        "@types/node": "^24.2.0",
        "@types/react": "^19.1.9",
        "@types/react-dom": "^19.1.7",
        "@vitejs/plugin-react": "^4.7.0",
        "eslint": "^9.32.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.3.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "sass-embedded": "^1.90.0",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.39.0",
        "unplugin-dts": "1.0.0-beta.6",
